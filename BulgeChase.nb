   RowBox[{"G2", "[", "m_", "]"}], "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i_", ",", "j_"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"c_", ",", "s_"}], "}"}]}], "]"}], ":=", " ", 
  RowBox[{"SparseArray", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"i", ",", "i"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "j"}], "}"}]}], "}"}], "->", "c"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"i", ",", "j"}], "}"}], "->", 
