 RowBox[{
  RowBox[{"U", "=", 
   RowBox[{"UpperTriangularize", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"V", "\[Transpose]"}], ".", "V"}], ",", "1"}], "]"}]}], " ", 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"T", "=", 
