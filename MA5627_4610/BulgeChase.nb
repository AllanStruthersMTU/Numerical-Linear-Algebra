       RowBox[{"V", "\[Transpose]"}]}]}], "-", 
     RowBox[{"V", ".", 
      SuperscriptBox["T", 
       RowBox[{"-", "1"}]], ".", 
      RowBox[{"V", "\[Transpose]"}]}], "+", 
     RowBox[{"V", ".", 
      SuperscriptBox["T", 
       RowBox[{"-", "T"}]], ".", 
      RowBox[{"V", "\[Transpose]"}], ".", "V", ".", 
      SuperscriptBox["T", 
       RowBox[{"-", "1"}]], ".", 
      RowBox[{"V", "\[Transpose]"}]}]}], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0f1756a5-c120-4367-9228-ba4be4e63890"],
 "\nso\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"V", ".", 
