     RowBox[{"V", "\[Transpose]"}]}], "=", 
    RowBox[{"V", ".", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["T", 
        RowBox[{"-", "T"}]], ".", "V", ".", 
       RowBox[{"V", "\[Transpose]"}], ".", 
       SuperscriptBox["T", 
        RowBox[{"-", "1"}]]}], ")"}], ".", 
     RowBox[{"V", "\[Transpose]"}]}]}], TraditionalForm]],ExpressionUUID->
  "ddf4e289-2f12-439a-8a03-e0831f1b4855"],
