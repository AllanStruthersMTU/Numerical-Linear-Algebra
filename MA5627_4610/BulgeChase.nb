Cell[TextData[{
 "For a ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Times]", "2"}], TraditionalForm]],ExpressionUUID->
  "3b614df9-42d2-45d5-9604-d75b2785fdd8"],
 " matrix we have\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"G", ".", 
     RowBox[{"(", "\[NoBreak]", GridBox[{
        {"a11", "a12"},
        {"a21", "a22"}
       }], "\[NoBreak]", ")"}], ".", 
     RowBox[{"G", "\[Transpose]"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {"c", 
          RowBox[{"-", "s"}]},
         {"s", "c"}
        }], "\[NoBreak]", ")"}], ".", 
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {"a11", "a12"},
         {"a21", "a22"}
        }], "\[NoBreak]", ")"}], ".", 
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {"c", "s"},
         {
          RowBox[{"-", "s"}], "c"}
        }], "\[NoBreak]", ")"}]}], "=", 
     TagBox[
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {
          RowBox[{
           RowBox[{"a11", " ", 
            SuperscriptBox["c", "2"]}], "+", 
           RowBox[{"s", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"-", "a12"}], " ", "c"}], "-", 
