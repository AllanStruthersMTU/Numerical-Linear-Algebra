            RowBox[{"i", ";;", 
             RowBox[{"-", "1"}]}], ",", "i"}], "\[RightDoubleBracket]"}], 
          "]"}]}], ";", " ", 
        RowBox[{
         RowBox[{"V", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{"i", ";;", 
            RowBox[{"-", "1"}]}], ",", "i"}], "\[RightDoubleBracket]"}], "=", 
         "v"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"A", "\[LeftDoubleBracket]", 
            RowBox[{
             RowBox[{"i", ";;", 
              RowBox[{"-", "1"}]}], ",", "j"}], "\[RightDoubleBracket]"}], 
           "=", 
           RowBox[{
            RowBox[{"A", "\[LeftDoubleBracket]", 
             RowBox[{
              RowBox[{"i", ";;", 
