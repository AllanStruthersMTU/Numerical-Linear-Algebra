Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Lanczos", "[", "A_", "]"}], "[", 
   RowBox[{"{", 
    RowBox[{"Q_", ",", 
     RowBox[{"{", 
      RowBox[{"\[Alpha]_", ",", "\[Beta]_"}], "}"}]}], "}"}], "]"}], ":=", 
  " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "m", ",", "n", ",", "w", ",", "\[Beta]New", ",", "\[Alpha]New", ",", 
      "qNew"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"m", ",", "n"}], "}"}], "=", 
      RowBox[{"Dimensions", "[", "Q", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"A", ".", 
       RowBox[{"Q", "\[LeftDoubleBracket]", 
        RowBox[{"All", ",", "n"}], "\[RightDoubleBracket]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Beta]New", "=", 
      RowBox[{"w", ".", 
       RowBox[{"Q", "\[LeftDoubleBracket]", 
        RowBox[{"All", ",", "n"}], "\[RightDoubleBracket]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"w", "-", 
       RowBox[{"\[Beta]New", " ", 
        RowBox[{"Q", "\[LeftDoubleBracket]", 
         RowBox[{"All", ",", "n"}], "\[RightDoubleBracket]"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]New", "=", 
