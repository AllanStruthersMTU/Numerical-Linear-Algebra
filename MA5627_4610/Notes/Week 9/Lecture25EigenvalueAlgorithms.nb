Cell["\<\
Matlab computes polynomial roots by building a matrix which has the same \
roots.  There are actually a whole bunch of these companion matrices for a \
given polynomial. The most common is implemented below\
\>", "Text",
 CellChangeTimes->{{3.8547965683342657`*^9, 3.8547969346463194`*^9}, {
  3.8547969999900937`*^9, 
  3.8547970055978146`*^9}},ExpressionUUID->"fac4b9b4-21d3-4221-869c-\
6bda8a6e6194"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CompanionMatrix", "[", "a_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"m", "=", 
       RowBox[{"Length", "[", "a", "]"}]}], ",", "A"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"A", "=", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Band", "[", 
           RowBox[{"{", 
            RowBox[{"2", ",", "1"}], "}"}], "]"}], "->", "1"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"m", "-", "1"}], ",", 
          RowBox[{"m", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"A", "\[LeftDoubleBracket]", 
       RowBox[{"All", ",", 
        RowBox[{"m", "-", "1"}]}], "\[RightDoubleBracket]"}], "=", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"a", "\[LeftDoubleBracket]", 
         RowBox[{"1", ";;", 
          RowBox[{"-", "2"}]}], "\[RightDoubleBracket]"}]}], "/", 
       RowBox[{"a", "\[LeftDoubleBracket]", 
        RowBox[{"-", "1"}], "\[RightDoubleBracket]"}]}]}], ";", 
     "\[IndentingNewLine]", "A"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

 CellChangeTimes->{{3.854796949660945*^9, 3.8547970556794033`*^9}, {
   3.854869375627665*^9, 3.8548694334955072`*^9}, {3.854869517637892*^9, 
