Cell[BoxData[
 RowBox[{
  RowBox[{"HessenbergReduction", "[", "AIn_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"A", "=", "AIn"}], ",", " ", 
      RowBox[{"m", "=", 
       RowBox[{"Length", "[", "AIn", "]"}]}], ",", "v", ",", "x"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "=", 
         RowBox[{"A", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{
            RowBox[{"k", "+", "1"}], ";;", "m"}], ",", "k"}], 
          "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v", "=", "x"}], ";", " ", 
        RowBox[{
         RowBox[{"v", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
         "=", 
         RowBox[{
          RowBox[{"v", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
           "+", 
          RowBox[{
           RowBox[{"Sign", "[", 
            RowBox[{
            "x", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
            "]"}], "*", 
           RowBox[{"Norm", "[", "x", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v", "=", 
         RowBox[{"Normalize", "[", "v", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"A", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{
            RowBox[{"k", "+", "1"}], ";;", "m"}], ",", 
           RowBox[{"k", ";;", "m"}]}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"A", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{
             RowBox[{"k", "+", "1"}], ";;", "m"}], ",", 
            RowBox[{"k", ";;", "m"}]}], "\[RightDoubleBracket]"}], "-", 
          RowBox[{"2", " ", 
           RowBox[{"KroneckerProduct", "[", 
            RowBox[{"v", ",", 
             RowBox[{"v", ".", 
              RowBox[{"A", "\[LeftDoubleBracket]", 
               RowBox[{
                RowBox[{
                 RowBox[{"k", "+", "1"}], ";;", "m"}], ",", 
                RowBox[{"k", ";;", "m"}]}], "\[RightDoubleBracket]"}]}]}], 
            "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"A", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{"1", ";;", "m"}], ",", 
           RowBox[{
            RowBox[{"k", "+", "1"}], ";;", "m"}]}], "\[RightDoubleBracket]"}],
          "=", 
         RowBox[{
          RowBox[{"A", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{"1", ";;", "m"}], ",", 
            RowBox[{
             RowBox[{"k", "+", "1"}], ";;", "m"}]}], 
           "\[RightDoubleBracket]"}], "-", 
          RowBox[{"2", " ", 
           RowBox[{"KroneckerProduct", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"A", "\[LeftDoubleBracket]", 
               RowBox[{
                RowBox[{"1", ";;", "m"}], ",", 
                RowBox[{
                 RowBox[{"k", "+", "1"}], ";;", "m"}]}], 
               "\[RightDoubleBracket]"}], ".", "v"}], ",", "v"}], 
            "]"}]}]}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", 
         RowBox[{"m", "-", "2"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Chop", "[", "A", "]"}]}]}], "]"}]}]], "Input",
