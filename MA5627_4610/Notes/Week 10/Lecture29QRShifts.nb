a94ec11be54f"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"m", "=", "12"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "m"}], "}"}]}], "]"}]}], ";", " ", 
  RowBox[{"A", "=", 
   RowBox[{"A", "+", 
    RowBox[{"A", "\[Transpose]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"T", "=", 
    RowBox[{"Chop", "[", 
     RowBox[{"HessenbergReduction", "[", "A", "]"}], "]"}]}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"S", "=", 
   RowBox[{"T", "\[LeftDoubleBracket]", 
    RowBox[{
     RowBox[{
      RowBox[{"m", "-", "1"}], ";;", "m"}], ",", 
     RowBox[{
      RowBox[{"m", "-", "1"}], ";;", "m"}]}], "\[RightDoubleBracket]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"t", ",", "s"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Det", "[", "S", "]"}], ",", 
     RowBox[{"Tr", "[", "S", "]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"M", "=", 
    RowBox[{
     RowBox[{"T", ".", "T"}], "-", 
     RowBox[{"s", " ", "T"}], "+", 
     RowBox[{"IdentityMatrix", "[", "m", "]"}]}]}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Q", ",", "R"}], "}"}], "=", 
   RowBox[{"QRDecomposition", "[", "M", "]"}]}], ";", 
  RowBox[{"Q", "=", 
   RowBox[{"Q", "\[Transpose]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"TabView", "[", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\"\<T\>\"", "->", 
     RowBox[{"MatrixPlot", "[", "T", "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<M\>\"", "->", " ", 
     RowBox[{"MatrixPlot", "[", "M", "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<Q\[Transpose].M.Q\>\"", "->", " ", 
     RowBox[{"MatrixPlot", "[", 
      RowBox[{"Chop", "[", 
       RowBox[{
        RowBox[{"Q", "\[Transpose]"}], ".", "M", ".", "Q"}], "]"}], "]"}]}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<Q\[Transpose].T.Q\>\"", "->", " ", 
     RowBox[{"MatrixPlot", "[", 
      RowBox[{"Chop", "[", 
       RowBox[{
        RowBox[{"Q", "\[Transpose]"}], ".", "T", ".", "Q"}], "]"}], "]"}]}]}],
    "\[IndentingNewLine]", "}"}], "]"}], "\[IndentingNewLine]"}], "Input",
