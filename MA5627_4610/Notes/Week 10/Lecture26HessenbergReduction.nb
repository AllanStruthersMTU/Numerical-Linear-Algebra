Cell[BoxData[{
 RowBox[{"Clear", "[", "Subscript", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"m", "=", "7"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dd", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     SubscriptBox["d", "i"], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "m"}], "}"}]}], "]"}]}], ";", 
