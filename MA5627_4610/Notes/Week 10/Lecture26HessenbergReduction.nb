 RowBox[{
  RowBox[{"MatrixForm", "[", "T", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Q", "=", 
   RowBox[{"IdentityMatrix", "[", "m", "]"}]}], ";", 
  RowBox[{
   RowBox[{"Q", "\[LeftDoubleBracket]", 
    RowBox[{
     RowBox[{"1", ";;", "4"}], ",", 
     RowBox[{"1", ";;", "4"}]}], "\[RightDoubleBracket]"}], "=", 
   FormBox[
    RowBox[{"(", "\[NoBreak]", GridBox[{
       {"p1", 
        RowBox[{"-", "p2"}], 
        RowBox[{"-", "p3"}], 
        RowBox[{"-", "p4"}]},
       {"p2", "p1", 
        RowBox[{"-", "p4"}], "p3"},
       {"p3", "p4", "p1", 
        RowBox[{"-", "p2"}]},
       {"p4", 
        RowBox[{"-", "p3"}], "p2", "p1"}
      }], "\[NoBreak]", ")"}],
    TraditionalForm]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Simplify", "[", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"Q", ".", "T"}], "]"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Hmm", "=", 
   RowBox[{"Q", ".", "T", ".", 
    RowBox[{"Q", "\[Transpose]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"A0", ",", "A1"}], "}"}], "=", 
   RowBox[{"CoefficientArrays", "[", 
    RowBox[{
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"Hmm", "\[LeftDoubleBracket]", 
        RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"p1", ",", "p2", ",", "p3", ",", "p4"}], "}"}], "}"}]}], 
      "]"}], ",", 
     RowBox[{"{", 
