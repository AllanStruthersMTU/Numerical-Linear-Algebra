 RowBox[{
  RowBox[{"Data", "=", 
   RowBox[{
    RowBox[{"Table", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"A", "=", 
        RowBox[{"RandomReal", "[", 
