       "\[IndentingNewLine]", 
       RowBox[{"Q", "=", 
        RowBox[{"HouseMat", "[", "a1", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"QA", "=", 
        RowBox[{"Q", ".", "A"}]}], ";", " ", 
       RowBox[{"QAQtr", "=", 
        RowBox[{"QA", ".", 
         RowBox[{"Q", "\[Transpose]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Norm", "[", 
