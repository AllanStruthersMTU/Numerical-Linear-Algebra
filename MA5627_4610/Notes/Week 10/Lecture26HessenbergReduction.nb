Cell[TextData[{
 "Householder reflections are good for introducing zeros in bulk. Remember \
Givens rotations\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {"p1", 
       RowBox[{"-", "p2"}]},
      {"p2", "p1"}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "6bcbae54-6d92-43b5-9ae7-5fae5c2c1343"],
 "\nand \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"p1", 
       RowBox[{"-", "p2"}], 
       RowBox[{"-", "p3"}], 
       RowBox[{"-", "p4"}]},
      {"p2", "p1", 
       RowBox[{"-", "p4"}], "p3"},
      {"p3", "p4", "p1", 
       RowBox[{"-", "p2"}]},
      {"p4", 
       RowBox[{"-", "p3"}], "p2", "p1"}
     }], "\[NoBreak]", ")"}], TraditionalForm]],ExpressionUUID->
  "b42e6795-99a4-48b2-ae51-2adaf1336a45"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"p1", "p2", "p3", "p4", "p5", "p6", "p7", "p8"},
      {
       RowBox[{"-", "p2"}], "p1", "p4", 
       RowBox[{"-", "p3"}], "p6", 
       RowBox[{"-", "p5"}], 
       RowBox[{"-", "p8"}], "p7"},
      {
       RowBox[{"-", "p3"}], 
       RowBox[{"-", "p4"}], "p1", "p2", "p7", "p8", 
       RowBox[{"-", "p5"}], 
       RowBox[{"-", "p6"}]},
      {
       RowBox[{"-", "p4"}], "p3", 
       RowBox[{"-", "p2"}], "p1", "p8", 
       RowBox[{"-", "p7"}], "p6", 
       RowBox[{"-", "p5"}]},
      {
       RowBox[{"-", "p5"}], 
       RowBox[{"-", "p6"}], 
       RowBox[{"-", "p7"}], 
       RowBox[{"-", "p8"}], "p1", "p2", "p3", "p4"},
      {
       RowBox[{"-", "p6"}], "p5", 
       RowBox[{"-", "p8"}], "p7", 
       RowBox[{"-", "p2"}], "p1", 
       RowBox[{"-", "p4"}], "p3"},
      {
       RowBox[{"-", "p7"}], "p8", "p5", 
       RowBox[{"-", "p6"}], 
       RowBox[{"-", "p3"}], "p4", "p1", 
       RowBox[{"-", "p2"}]},
      {
       RowBox[{"-", "p8"}], 
       RowBox[{"-", "p7"}], "p6", "p5", 
       RowBox[{"-", "p4"}], 
       RowBox[{"-", "p3"}], "p2", "p1"}
     }], "\[NoBreak]", ")"}], TraditionalForm]],ExpressionUUID->
  "c389c1da-af66-4f9b-8555-ad62458c7170"],
 "\nare orthogonal for unit vectors. "
}], "Text",
