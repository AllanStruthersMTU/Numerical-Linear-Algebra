           OverscriptBox["a", "~"], 
           RowBox[{"2", ",", "2"}]], 
          SubscriptBox[
           OverscriptBox["a", "~"], 
           RowBox[{"2", ",", "3"}]]},
         {"0", "0", 
          SubscriptBox[
           OverscriptBox["a", "~"], 
