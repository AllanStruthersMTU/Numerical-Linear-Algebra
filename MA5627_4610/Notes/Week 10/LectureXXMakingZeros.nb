  FormBox[
   SubscriptBox["G", "4"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2b3457a8-c99c-48db-ac39-8151b522bd11"],
 " rotations\n\t",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["G", "4"], "=", 
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"p1", "p2", "p3", "p4"},
      {
       RowBox[{"-", "p2"}], "p1", "p4", 
       RowBox[{"-", "p3"}]},
      {
       RowBox[{"-", "p3"}], 
       RowBox[{"-", "p4"}], "p1", "p2"},
      {
       RowBox[{"-", "p4"}], "p3", 
       RowBox[{"-", "p2"}], "p1"}
     }], "\[NoBreak]", ")"}]}]],
  CellChangeTimes->{{3.8508996725220404`*^9, 3.8508996732329416`*^9}, {
