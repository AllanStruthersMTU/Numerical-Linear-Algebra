     TagBox[
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {
          SubscriptBox[
           OverscriptBox["a", "~"], 
           RowBox[{"1", ",", "1"}]], 
          SubscriptBox[
           OverscriptBox["a", "~"], 
           RowBox[{"1", ",", "2"}]], 
