        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
           "b", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], "-", 
           RowBox[{"Sum", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"R", "\[LeftDoubleBracket]", 
               RowBox[{"j", ",", "k"}], "\[RightDoubleBracket]"}], 
              RowBox[{
              "x", "\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]}], 
             ",", 
             RowBox[{"{", 
              RowBox[{"k", ",", 
               RowBox[{"j", "+", "1"}], ",", "m"}], "}"}]}], "]"}]}], ")"}], 
