Cell[TextData[{
 "For an ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "\[Times]", "n"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4c00559b-bab3-4b3c-a577-22b8d5b62126"],
 " matrix ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "82d91eba-4486-4eb0-b229-e6af942ba2f2"],
 " the adjoint ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "*"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a3c10b88-bf1b-4e3e-8437-b9e04b437a6e"],
 " is the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[Times]", "m"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c10c0697-590b-4ef7-ad2e-10f153fec20a"],
 " matrix obtained by interchanging rows and columns of the complex conjugate \
of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c4d4d3d5-4f06-4d46-9865-86626d28f1d1"],
 ".  In other words\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      SuperscriptBox["A", "*"], ")"}], 
     RowBox[{"i", ",", "j"}]], "=", 
    SubscriptBox[
     OverscriptBox["A", "_"], 
     RowBox[{"j", ",", "i"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b4e3b2b3-8422-48bb-9cb4-4ffd2eb2db65"],
 "  "
}], "Text",
 CellChangeTimes->{{3.846670950587816*^9, 
  3.846671114385092*^9}},ExpressionUUID->"0af2a1db-c827-410f-baf3-\
079593ddc538"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalC]", "m"], TraditionalForm]],
  ExpressionUUID->"14614337-1ef5-4a29-ad5b-abc16d30b277"],
 " comes with an inner product\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"<", "x"}], ",", 
    RowBox[{
     RowBox[{"y", ">"}], " ", "=", 
     RowBox[{
      RowBox[{
       OverscriptBox["x", "_"], ".", "y"}], "=", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "1"}], "m"], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox[
          OverscriptBox["x", "_"], "j"], 
         SubscriptBox["y", "j"]}], ")"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"932207e7-26ce-4d9c-adb3-fc7ee6e229e1"],
 "\nwhich is just the familiar calculus inner product if we are in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "m"], TraditionalForm]],
  ExpressionUUID->"9b43083c-4d99-4922-91a1-7e41afc8b51a"],
 ".   The norm is just ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"||", "x", "||"}], "=", 
    RowBox[{
     SqrtBox[
      RowBox[{"<", 
       RowBox[{"x", ".", "x"}], ">"}]], "."}]}], TraditionalForm]],
  ExpressionUUID->"1b951549-1b32-4bff-9fb0-08bdcc5f7cac"],
 "  In ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "m"], TraditionalForm]],
  ExpressionUUID->"7f9a61b2-b166-4982-940f-63b5f7cc77c4"],
 " we have the familiar formula for the angle ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]],ExpressionUUID->
  "8c22df7d-44ce-4864-8b43-4de86820e808"],
 " between two vectors \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Cos", "[", "\[Alpha]", "]"}], "=", 
    RowBox[{"<", 
     RowBox[{"x", ".", "y"}], ">", 
     FractionBox[
      RowBox[{"<", 
       RowBox[{"x", ".", "y"}], ">"}], 
      RowBox[{"||", "x", "||", "||", "y", "||"}]]}]}], TraditionalForm]],
  ExpressionUUID->"59169271-62f9-48e7-a6c7-707ee4af88b2"],
 "\nWarning, if x and y are single column matrices then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"<", "x"}], ",", 
    RowBox[{
     RowBox[{"y", ">"}], " ", "=", 
     RowBox[{
      SuperscriptBox["x", "*"], ".", "y"}]}]}], TraditionalForm]],
  ExpressionUUID->"779efef3-673b-4cd2-9d6d-5d5effbdd6f7"],
 " which is a common matlab syntax.  Other software defines and stores \
vectors as vectors rather than extremely skinny matrices"
}], "Text",
